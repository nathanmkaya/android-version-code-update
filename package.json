{
  "name": "android-version-code-update",
  "version": "0.1.2",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanmkaya/android-version-code-update.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "googleapis": "^65.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.12.5",
    "@types/babel__parser": "^7.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.10.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.13.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.9.0",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.1.2"
  }
}
